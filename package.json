{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Este es un curso de webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js --mode development",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js --mode development",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode development",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --mode development",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js --mode development"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@mavalderramav",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0"
  }
}
